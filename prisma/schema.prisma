datasource db {
  provider = "postgresql"
  url      = "postgresql://prisma:prisma@postgres/"
}

generator photon {
  provider = "photonjs"
}

model User {
  id                String   @default(cuid()) @id
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
  name              String   @unique
  email             String?
  githubAccessToken String?
}