datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

generator photon {
  provider = "photonjs"
}

model User {
  id                String   @default(cuid()) @id
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
  name              String   @unique
  email             String?
  githubAccessToken String?
}